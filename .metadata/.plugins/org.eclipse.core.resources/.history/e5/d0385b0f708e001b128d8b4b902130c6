#include <FuncLibPart3.h>
#include "msp.h"

void main(void)
{
    WDT_A->CTL = WDT_A_CTL_PW | WDT_A_CTL_HOLD;		// stop watchdog timer
    LCD_init();
    adcsetup();
    write_command(0x85);        //begin on line 1
    PrintChar();

    SysTick_Init_interrupt();
    __enable_interrupt();
    while(1){
    }
}
