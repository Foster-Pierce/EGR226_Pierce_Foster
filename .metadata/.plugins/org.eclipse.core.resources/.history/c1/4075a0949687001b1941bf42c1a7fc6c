#include <TimerLib_part2.h>
#include "msp.h"

void Setup_Init(void){
        //P2.7 init TimerA 0.4
        P2SEL1 |= BIT7;
        P2SEL0 = 0x00;
        P2DIR |= BIT7;
}
void MotorSpeed(void){
    int duty,x=1;
    duty=75000*x;
    TIMER_A0->CCR[0] = 75000;
    TIMER_A0->CCR[4] = duty;
    TIMER_A0->CCTL[4] = 0xE0;
    TIMER_A0->CTL = 0x0214;
}
