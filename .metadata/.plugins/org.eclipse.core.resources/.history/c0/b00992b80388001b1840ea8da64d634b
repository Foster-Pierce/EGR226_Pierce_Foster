#include <TimerLib_part3.h>
#include "msp.h"
void main(void)
{
	WDT_A->CTL = WDT_A_CTL_PW | WDT_A_CTL_HOLD;		// stop watchdog timer
	int press;
	double DC=0.5;
	while(1){
	        press = Keypad_Read();
	        DC=Press_Convert(press);
	        if(pressed>=1)
	            MotorSpeed(DC);
	        SysTick_delay(10);
	    }
}
