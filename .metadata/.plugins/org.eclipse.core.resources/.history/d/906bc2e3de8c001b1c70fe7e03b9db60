#include <TimerLib_part2.h>
#include "msp.h"
volatile int n=0;

void Setup_Init(void){

    P4SEL1 &= ~BIT2; // configure P4.2 as simple I/O
    P4SEL0 &= ~BIT2;
    P4DIR |= BIT2;
}

void SysTick_Init_interrupt(void);{
    int i;
    SysTick->CTRL=0;
    SysTick->LOAD=(1500000-1);
    SysTick->VAL=0;
    SysTick->CTRL=0x07;
}

void SysTick_Handler (void) {
    CaseManager(n)
}

void CaseManager (int n){

    switch(n) {
    case 0 :

        break;
    case 1 :

        break;
    case 2 :

        break;
    case 3 :

        break;
    case 4 :

        break;
    case 5 :

        break;
    case 6 :

        break;
    case 7 :

        break;
    case 8 :

        break;
    case 9 :

        break;
    default :
        n=0;
    }
}
