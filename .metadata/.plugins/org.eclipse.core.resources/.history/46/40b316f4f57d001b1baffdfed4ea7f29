#include "msp.h"
#include "LCD_preset.h"

void main(void)
{
    WDT_A->CTL = WDT_A_CTL_PW | WDT_A_CTL_HOLD;		// stop watchdog timer

    int arr[][4]={{0x50,0x49,0x45,0x52,0x43,0x45},
                   {0x46,0x4F,0x53,0x54,0x45,0x52},
                   {0x45,0x47,0x52},
                   {0x32,0x32,0x36}};

    LCD_init();
    PrintChar(arr, 0);
    PrintChar(arr, 1);
    PrintChar(arr, 2);
    PrintChar(arr, 3);
    while(1){
        ;
    }
}



