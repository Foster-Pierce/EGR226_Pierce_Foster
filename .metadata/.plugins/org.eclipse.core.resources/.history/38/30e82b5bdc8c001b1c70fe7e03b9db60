#include <TimerLib_part2.h>
#include "msp.h"
volatile int n=0;

void Setup_Init(void){

    P4SEL1 &= ~BIT2; // configure P4.2 as simple I/O
    P4SEL0 &= ~BIT2;
    P4DIR |= BIT2;
}

void SysTick_Init_interrupt(void);{
    int i;
    SysTick->CTRL=0;
    SysTick->LOAD=(1500000-1);
    SysTick->VAL=0;
    SysTick->CTRL=0x07;
}

void SysTick_Handler (void) {
CaseManager()
if(n==9)
    n=0;
else
    n++;
}

void CaseManager (int n){

    switch(n) {
          case 0 :
             printf("Excellent!\n" );
             break;
          case 1 :
          case 2 :
             printf("Well done\n" );
             break;
          case 3 :
             printf("You passed\n" );
             break;
          case 4 :
             printf("Better try again\n" );
             break;
          default :
             printf("Invalid grade\n" );
       }
}
